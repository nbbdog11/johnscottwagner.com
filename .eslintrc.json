{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react-hooks"
    ],
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "jsx-a11y/anchor-is-valid": 0,
        "react-hooks/rules-of-hooks": "error"
    },
    "globals": {
        "graphql": false,
        "window": false
    }
}